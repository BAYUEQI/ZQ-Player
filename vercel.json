{
  "rewrites": [
    {
      "source": "/:path",
      "destination": "/index.html"
    },
    {
      "source": "/api/:apiurl*",
      "destination": "https://musicapi.qizou.dpdns.org/:apiurl*"
    },
    {
      "source": "/api/ttml/:ttmlurl*",
      "destination": "https://lyrics.qizou.dpdns.org/:ttmlurl*"
    }
  ],
  "outputDirectory": "out/renderer"
}
